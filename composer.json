{
    "name": "tsufeki/blanche-json-rpc",
    "description": "Async JSON-RPC library for Recoil PHP",
    "license": "MIT",
    "authors": [
        {
            "name": "tsufeki",
            "email": "tsufeki@ymail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "recoil/api": "^1",
        "tsufeki/kayo-json-mapper": "^0.3",
        "psr/log": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "phpstan/phpstan": "^0.10",
        "phpstan/phpstan-phpunit": "^0.10",
        "recoil/react": "^1"
    },
    "autoload": {
        "psr-4": {
            "Tsufeki\\BlancheJsonRpc\\": "src/Tsufeki/BlancheJsonRpc/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Tsufeki\\BlancheJsonRpc\\": "tests/Tsufeki/BlancheJsonRpc/"
        }
    }
}
